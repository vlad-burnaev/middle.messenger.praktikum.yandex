{
    "env": {
        "browser": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": false,
        "import/prefer-default-export": false,
        "default-case-last": false,
        "default-param-last": false,
        "grouped-accessor-pairs": false,
        "no-constructor-return": false,
        "no-nonoctal-decimal-escape": false,
        "prefer-regex-literals": false,
        "no-dupe-else-if": false,
        "no-import-assign": false,
        "no-loss-of-precision": false,
        "no-promise-executor-return": false,
        "no-setter-return": false,
        "no-unreachable-loop": false,
        "no-unsafe-optional-chaining": false,
        "no-useless-backreference": false,
        "function-call-argument-newline": "off",
        "prefer-exponentiation-operator": false,
        "no-restricted-exports": false,
        "no-underscore-dangle": "off",
        "prefer-destructuring": "off",
        "no-param-reassign": "off",
        "no-useless-constructor": "off",
        "array-callback-return": "off",
        "max-len": ["error", { "code": 120 }]
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".ts"]
            }
        }
    }
}
