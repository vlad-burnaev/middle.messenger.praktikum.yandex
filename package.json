    "lint:fix": "yarn lint:css:fix && yarn lint:js:fix"
  },
  "keywords": [],
  "author": "Vlad Burnaev <vlad.burn325@gmail.com>",
  "license": "ISC",
  "alias": {
    "components": "~/src/components",
    "utils": "~/src/utils",
    "helpers": "~/src/helpers",
    "style": "~/src/style",
    "pages": "~/src/pages",
    "static": "~/static",
    "api": "~/src/api",
    "core": "~/src/core",
    "services": "~/src/services",
    "const": "~/src/const",
    "store": "~/src/store"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
