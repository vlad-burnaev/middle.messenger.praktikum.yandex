{
  "name": "messenger",
  "version": "1.2.7",
  "description": "",
  "keywords": [],
  "author": "Vlad Burnaev <vlad.burn325@gmail.com>",
  "license": "ISC",
  "alias": {
    "components": "~/src/components",
    "utils": "~/src/utils",
    "helpers": "~/src/helpers",
    "style": "~/src/style",
    "pages": "~/src/pages",
    "static": "~/static",
    "api": "~/src/api",
    "core": "~/src/core",
    "services": "~/src/services",
    "const": "~/src/const",
    "store": "~/src/store"
  },
  "scripts": {
    "dev": "webpack serve --mode=development --progress",
    "start": "node server.js",
    "build": "webpack",
    "build:prod": "webpack --config webpack.config.prod.js",
    "test": "jest",
    "lint": "npm-run-all --parallel lint:css lint:js",
    "lint:css": "stylelint **/*.scss",
    "lint:css:fix": "stylelint **/*.scss --fix",
    "lint:js": "eslint --ext .js,.ts,.tsx src/",
    "lint:js:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "lint:fix": "npm-run-all --parallel lint:css:fix lint:js:fix",
    "precommit": "npm-run-all --parallel lint test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@types/jest": "27.4.1",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "babel-jest": "28.1.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "eslint": "8.7.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-svg-transformer": "1.0.0",
    "mini-css-extract-plugin": "2.6.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.0.1",
    "postcss-loader": "7.0.0",
    "postcss-modules": "4.3.1",
    "postcss-nested": "5.0.6",
    "regenerator-runtime": "0.13.9",
    "sass": "1.45.1",
    "sass-loader": "13.0.0",
    "stylelint": "14.5.1",
    "stylelint-config-standard": "17.0.0",
    "stylelint-config-standard-scss": "4.0.0",
    "stylelint-order": "0.7.0",
    "terser-webpack-plugin": "5.3.3",
    "ts-jest": "28.0.2",
    "ts-loader": "9.3.0",
    "ts-node": "10.5.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.5.5",
    "webpack": "5.73.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.2",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "express": "4.17.2",
    "handlebars": "4.7.7",
    "nanoid": "3.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.ts"
    ],
    "transform": {
      "^.+\\.svg$": "<rootDir>/src/tests/svgTransform.ts",
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(scss|png)$": "identity-obj-proxy",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  }
}
