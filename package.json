{
  "name": "messenger",
  "version": "1.2.1",
  "description": "",
  "scripts": {
    "dev": "parcel src/index.html --no-cache -p 3000",
    "build": "parcel build src/index.html",
    "start": "node server.js",
    "lint:css": "stylelint **/*.pcss",
    "lint:css:fix": "stylelint **/*.pcss --fix",
    "lint:js": "eslint --ext .js,.ts,.tsx src/",
    "lint:js:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:fix": "yarn lint:css:fix && yarn lint:js:fix"
  },
  "keywords": [],
  "author": "Vlad Burnaev <vlad.burn325@gmail.com>",
  "license": "ISC",
  "alias": {
    "components": "~/src/components",
    "utils": "~/src/utils",
    "helpers": "~/src/helpers",
    "style": "~/src/style",
    "pages": "~/src/pages",
    "static": "~/static",
    "api": "~/src/api",
    "core": "~/src/core",
    "services": "~/src/services",
    "const": "~/src/const",
    "store": "~/src/store"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@parcel/babel-preset-env": "^2.5.0",
    "@parcel/config-default": "2.1.1",
    "@parcel/transformer-sass": "2.1.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "babel-jest": "^28.1.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "8.7.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-svg-transformer": "^1.0.0",
    "parcel": "2.0.1",
    "postcss-modules": "4.3.1",
    "postcss-nested": "5.0.6",
    "regenerator-runtime": "^0.13.9",
    "sass": "1.45.1",
    "stylelint": "14.5.1",
    "stylelint-config-standard": "17.0.0",
    "stylelint-order": "0.7.0",
    "ts-jest": "^28.0.2",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "express": "4.17.2",
    "handlebars": "4.7.7",
    "nanoid": "3.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.ts"
    ],
    "transform": {
      "^.+\\.svg$": "<rootDir>/src/tests/svgTransform.ts",
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(scss|svg|png)$": "identity-obj-proxy",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  }
}
